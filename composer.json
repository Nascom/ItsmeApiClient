{
  "name": "nascom/itsme-api-client",
  "description": "PHP client to connect to the itsme API ",
  "type": "library",
  "authors": [{
    "name": "Brecht Ceyssens",
    "role": "Lead Developer"
  }],
  "keywords": [
    "itsme",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Nascom\\ItsmeApiClient\\":  ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nascom\\ItsmeApiClient\\Tests\\": ["tests/"]
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4"
  }
}
